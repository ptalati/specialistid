          }
        },
        "accordion_qVzwRz": {
          "type": "accordion",
          "settings": {
            "accordion_title": "What is your return policy?",
            "accordion_content": "<p>We accept returns of unused items within 30 days of delivery. See our <a href=\"https://www.specialistid.com/pages/returns\" title=\"https://www.specialistid.com/pages/returns\">Returns & Exchanges</a> page for details.</p>"
          }
        },
        "accordion_Xyprfk": {
